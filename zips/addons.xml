<addons>
  <addon id="plugin.program.dawgwizard" name="[B][COLOR red]Dawg[COLOR snow]Wizard [/COLOR][/B]" version="4.20" provider-name="J-Dawg">
    <requires>
      <import addon="xbmc.python" version="3.0.0" />
      <import addon="script.module.debridmgr" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
      <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" />
    <extension point="xbmc.addon.metadata">
      <summary lang="en_GB">Dawg Wizard for updating Dawg build in Kodi</summary>
      <description lang="en_GB">Dawg Wizard</description>
      <disclaimer lang="en_GB">Them Dawgs is Hell</disclaimer>
      <language>en</language>
      <platform>all</platform>
      <license>GPL-2.0-or-later</license>
      <forum />
      <website />
      <email />
      <source />
      <news />
      <assets>
        <icon>resources/icon.png</icon>
        <fanart>resources/fanart.jpg</fanart>
        <banner />
        <clearlogo />
        <screenshot />
      </assets>
    </extension>
  </addon>
  <addon id="repository.repodawg" name="RepoDawg Repository" version="1.0.12" provider-name="RepoDawg">
    <extension point="xbmc.addon.repository" name="RepoDawg">
      <info compressed="false">https://raw.githubusercontent.com/jason247g/RepoDawg/master/addons.xml</info>
      <checksum>https://raw.githubusercontent.com/jason247g/RepoDawg/master/addons.xml.md5</checksum>
      <datadir zip="true">https://raw.githubusercontent.com/jason247g/RepoDawg/master/zips/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary>RepoDawg</summary>
      <description>RepoDawg add-on repository</description>
      <platform>all</platform>
    </extension>
  </addon>
  <addon id="script.module.dbview" name="dbviewer" version="1.0.3" provider-name="Zaxxon709">
    <extension point="xbmc.python.pluginsource" library="default.py" />
    <extension point="xbmc.python.module" library="resources" />
    <extension point="xbmc.addon.metadata">
      <summary lang="en">View Authorized Accounts</summary>
      <description lang="en">Debrid Manager Dependency</description>
      <platform>all</platform>
      <reuselanguageinvoker>false</reuselanguageinvoker>
      <source>https://</source>
    </extension>
  </addon>
  <addon id="script.module.debridmgr" version="1.0.4" name="Debrid Manager" provider-name="Zaxxon709">
    <requires>
      <import addon="xbmc.python" version="3.0.0" />
      <import addon="script.module.requests" version="2.19.1" />
      <import addon="script.module.dbview" version="1.0.3" />
    </requires>
    <extension point="xbmc.service" library="lib/startup.py" start="startup" />
    <extension point="xbmc.python.script" library="lib/default.py">
      <provides>executable</provides>
    </extension>
    <extension point="xbmc.python.module" library="lib" />
    <extension point="xbmc.addon.metadata">
      <reuselanguageinvoker>false</reuselanguageinvoker>
